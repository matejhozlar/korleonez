{
  "name": "korleonez",
  "version": "1.0.0",
  "description": "",
  "main": "server.ts",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc && npm run import:fix && npm run copy:assets",
    "start": "node dist/server.js",
    "typecheck": "tsc --noEmit",
    "copy:assets": "copyfiles -u 1 \"src/**/*.html\" \"src/**/*.png\" \"src/**/*.jpg\" \"src/**/*.svg\" dist/",
    "import:fix": "node fix-imports.js",
    "structure": "node generate-structure.js",
    "env:gen": "node src/scripts/env/env-gen.js",
    "env:find": "node src/scripts/env/env-find.js",
    "import:find": "node src/scripts/dependency/import-find.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run --dir src/tests/unit",
    "test:queries": "vitest run src/tests/units/db/queries tests/integration/db/queries",
    "test:queries:unit": "vitest run src/tests/unit/db/queries",
    "test:queries:integration": "vitest run src/tests/integration/db/queries",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/express-rate-limit": "^5.1.3",
    "@types/node": "^24.10.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.14"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "discord.js": "^14.25.1",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "fast-glob": "^3.3.3"
  }
}
